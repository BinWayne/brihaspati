$page.setTitle("Admin Home | User Management | Proxy User")
$!data.getMessage()
<table bgcolor="$ui.menuColor" width=100%>
<tr>
	<td><a href=$link.setPage("Index.vm") style=text-decoration:none>Admin's Home </a> | Proxy User</td>
</tr>
</table>
<form name=frm method="post" action="$link.setPage("call,UserMgmt_Admin,ProxyUser.vm").setAction("AddProxyUser")">
<table bgcolor="$ui.tableColor" width=100%>
<tr>
	<td colspan="2">
	<b>
		#if(( $data.getUser().getTemp("lang" ).toString())=="hindi")
			Proxy User van Details  Uploaden
		#else
			Enter Proxy User Details 
		#end
	</b></td>
</tr>
<tr bgcolor="$ui.menuColor">
	<td colspan=2><b>Proxy User Naam <font color=red>*</font></b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <input type="text" size=25 name="UNAME" value=""></td>
	##formCell ("Proxy User Naam <font color=red>*</font>" "UNAME" "")
</tr>
<tr>
	<td colspan=2><b>Wachtwoord</b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
	<input type="text" size=25 name="PASSWD" value=""></td>
	##formPasswordCell ("Wachtwoord" "PASSWD" "")
</tr>
<tr bgcolor="$ui.menuColor">
	<td colspan=2>
	<input type="hidden" name="actionName">
	<input type="button" name="eventSubmit_doRegister" value="Registreer" onClick="checkField(document.frm,this);"/> 
	</td>
</tr>
</table>
</form>



<SCRIPT LANGUAGE="javascript">
	function checkField(frm,field)
	{
		if(frm.UNAME.value!="") 
		{
				frm.actionName.value=field.name;
				frm.submit();
		}
		else
		{
		        alert("Please fill the Mandatory(*) Fields !!");
		}
	}
</SCRIPT>
