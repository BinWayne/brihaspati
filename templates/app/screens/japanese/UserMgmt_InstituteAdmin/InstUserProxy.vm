<form name=frm method="post" action="$link.setPage("call,UserMgmt_InstituteAdmin,InstUserProxy.vm").setAction("InstituteProxyUser")">
<table bgcolor="$ui.tableColor" width=100%>
<tr>
	<td colspan="2">
	<b>
		#if(( $data.getUser().getTemp("lang" ).toString())=="hindi")
			プロキシのユーザ の 詳細 アップロード
		#else
			Enter プロキシのユーザ 詳細
		#end
	</b></td>
</tr>
<tr bgcolor="$ui.menuColor">
	<td colspan=2><b>プロキシのユーザ 名前 <font color=red>*</font></b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <input type="text" size=25 name="UNAME" value=""></td>
	##formCell ("プロキシのユーザ 名前 <font color=red>*</font>" "UNAME" "")
</tr>
<tr>
	<td colspan=2><b>パスワード</b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
	<input type="text" size=25 name="PASSWD" value=""></td>
	##formPasswordCell ("パスワード" "PASSWD" "")
</tr>
<tr bgcolor="$ui.menuColor">
	<td colspan=2>
	<input type="hidden" name="actionName">
	<input type="button" name="eventSubmit_doRegister" value="登録" onClick="checkField(document.frm,this);"/> 
	</td>
</tr>
</table>
</form>



<SCRIPT LANGUAGE="javascript">
	function checkField(frm,field)
	{
		if(frm.UNAME.value!="") 
		{
				frm.actionName.value=field.name;
				frm.submit();
		}
		else
		{
		        alert("Please fill the Mandatory(*) Fields !!");
		}
	}
</SCRIPT>
